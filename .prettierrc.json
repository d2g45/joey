{
    "trailingComma": "es5",
    "tabs": false,
    "tabWidth": 4,
    "semi": true,
    "singleQuote": false,
    "plugins": ["@zackad/prettier-plugin-twig-melody"],
    "twigMultiTags": [
        "nav,endnav",
        "switch,case,default,endswitch",
        "ifchildren,endifchildren",
        "cache,endcache",
        ".js,endjs"
    ]
}
