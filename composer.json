{
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "c10d/craft-recaptcha": "3.0.1",
    "clubstudioltd/craft-inline-svg": "2.1.0",
    "craftcms/ckeditor": "4.3.1",
    "craftcms/cms": "^5.0.0",
    "craftcms/contact-form": "3.1.0",
    "craftcms/contact-form-honeypot": "2.1.0",
    "ether/seo": "5.0.0",
    "hybridinteractive/craft-contact-form-extensions": "5.0.0",
    "nystudio107/craft-vite": "5.0.1",
    "simplygoodwork/craft-block-usage": "2.0.1",
    "spicyweb/craft-embedded-assets": "5.2.0",
    "verbb/field-manager": "4.0.3",
    "verbb/super-table": "4.0.2",
    "verbb/tablemaker": "5.0.3",
    "vlucas/phpdotenv": "^5.4.0"
  },
  "require-dev": {
    "craftcms/generator": "^2.0.0",
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.2"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example.dev', '.env');\""
    ]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    }
  ]
}